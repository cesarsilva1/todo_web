<div align="left">
<h3>Your ToDo's</h3>
<ul>
  <% @lists.each do |list| %>
      <% if (list.shared == false) %>
      <li>
      	<%= link_to list.name, list_path(list) %>
        <%= link_to "#", edit_list_path(list) %>
    </li>
    <%end%>
  <% end %>
</ul>
<h3> Shared ToDo's</h3>
<ul>
	<% @lists.each do |list| %>
      <% if (list.shared == true) %>
      <li>
      	<%= link_to list.name, list_path(list) %>
        <%= link_to "#", edit_list_path(list) %>
      </li>
      <%end%>
  <% end %>
</ul>
<h4>
<%= link_to "New ToDo list...", new_list_path%>
</h4>
</div>